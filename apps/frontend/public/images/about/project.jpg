<svg xmlns="http://www.w3.org/2000/svg" width="600" height="800" viewBox="0 0 600 800">
  <!-- Background -->
  <rect width="600" height="800" fill="#f5f5f5"/>

  <!-- Decorative Pattern -->
  <g opacity="0.3">
    <rect x="50" y="100" width="120" height="120" fill="#e0e0e0" rx="8"/>
    <rect x="430" y="580" width="120" height="120" fill="#e0e0e0" rx="8"/>
    <rect x="200" y="350" width="80" height="80" fill="#e0e0e0" rx="8"/>
  </g>

  <!-- Main Icon (Weaving Loom) -->
  <g transform="translate(300, 280)">
    <!-- Loom frame -->
    <rect x="-120" y="-80" width="240" height="200" fill="none" stroke="#4b5563" stroke-width="8"/>
    <!-- Vertical threads -->
    <line x1="-100" y1="-80" x2="-100" y2="120" stroke="#000000" stroke-width="2"/>
    <line x1="-60" y1="-80" x2="-60" y2="120" stroke="#000000" stroke-width="2"/>
    <line x1="-20" y1="-80" x2="-20" y2="120" stroke="#000000" stroke-width="2"/>
    <line x1="20" y1="-80" x2="20" y2="120" stroke="#000000" stroke-width="2"/>
    <line x1="60" y1="-80" x2="60" y2="120" stroke="#000000" stroke-width="2"/>
    <line x1="100" y1="-80" x2="100" y2="120" stroke="#000000" stroke-width="2"/>
    <!-- Woven pattern -->
    <rect x="-110" y="0" width="220" height="30" fill="#e0e0e0" opacity="0.8"/>
    <rect x="-110" y="40" width="220" height="30" fill="#4b5563" opacity="0.6"/>
  </g>

  <!-- Text Label -->
  <text x="300" y="550" font-family="Arial, sans-serif" font-size="24" fill="#000000" text-anchor="middle" font-weight="bold">
    Le Projet
  </text>
  <text x="300" y="590" font-family="Arial, sans-serif" font-size="16" fill="#4b5563" text-anchor="middle">
    Atelier placeholder
  </text>
  <text x="300" y="620" font-family="Arial, sans-serif" font-size="14" fill="#4b5563" text-anchor="middle" opacity="0.7">
    600 x 800
  </text>
</svg>
